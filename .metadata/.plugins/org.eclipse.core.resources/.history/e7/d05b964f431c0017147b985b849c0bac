package org.rachel.graph.informationflow;

import java.util.Date;

public class InfoflowEdge {
    String id;
    String fromId;
    String toId;
    String description; 
    int numberOfMessagePerDay;
    Date createdDate;
    
     
    public InfoflowEdge(String id, String fromId,String toId,String description,     int numberOfMessagePerDay,
    Date createdDate){
      this.id=id;	
      this.fromId=fromId;	
      this.toId=id;
      this.description =description;
      this.numberOfMessagePerDay=numberOfMessagePerDay;
    }
	public String getId() {
		return id;
	}
	public void setId(String id) {
		this.id = id;
	}
	public String getFromId() {
		return fromId;
	}
	public void setFromId(String fromId) {
		this.fromId = fromId;
	}
	public String getToId() {
		return toId;
	}
	public void setToId(String toId) {
		this.toId = toId;
	}
	public String getDescription() {
		return description;
	}
	public void setDescription(String description) {
		this.description = description;
	}
	public int getNumberOfMessagePerDay() {
		return numberOfMessagePerDay;
	}
	public void setNumberOfMessagePerDay(int numberOfMessagePerDay) {
		this.numberOfMessagePerDay = numberOfMessagePerDay;
	}
	public Date getCreatedDate() {
		return createdDate;
	}
	public void setCreatedDate(Date createdDate) {
		this.createdDate = createdDate;
	}
}
